{
    "name": "symplify/skipper",
    "description": "Skip files by rule class, directory, file or fnmatch",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3.x-dev",
        "symfony/finder": "^5.3",
        "symfony/filesystem": "^5.3",
        "symplify/package-builder": "^9.4.21",
        "symplify/symplify-kernel": "^9.4.21",
        "symplify/smart-file-system": "^9.4.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Skipper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Skipper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
